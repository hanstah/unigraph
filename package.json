{
  "name": "unigraph",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "vite",
    "start": "vite",
    "build": "tsc && node --max-old-space-size=8192 node_modules/.bin/vite build",
    "preview": "vite preview",
    "test": "npx jest --coverage --silent",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx src/",
    "lint-fix": "eslint --ext .js,.jsx,.ts,.tsx src/ --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "doc-server": "cd docs && bundle exec jekyll serve --livereload",
    "doc-server-build": "cd docs && bundle exec jekyll build",
    "doc-server-clean": "cd docs && bundle exec jekyll clean",
    "generate-docs": "node scripts/generate-docs-structure.js",
    "generate-markdowns": "node scripts/generate-markdowns-structure.js",
    "generate-structures": "npm run generate-docs && npm run generate-markdowns",
    "generate-ast": "npx ts-node scripts/generateInterfaceAST.ts",
    "dev:app-shell": "cd ../app-shell && npm run build-lib",
    "watch:app-shell": "cd ../app-shell && npm run build-lib && fswatch -o src/ | xargs -n1 -I{} npm run build-lib",
    "dev:full": "concurrently \"npm run dev\" \"npm run watch:app-shell\"",
    "clean": "rm -rf node_modules package-lock.json dist .vite coverage .nyc_output",
    "clean:full": "npm run clean && npm cache clean --force",
    "clean:install": "npm run clean && npm install"
  },
